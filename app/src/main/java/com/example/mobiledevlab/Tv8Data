package com.example.mobiledevlab

data class AllArticles(
    val results: List<Result>
)

data class Result(
    val id: Int,
    val slug: String,
    val status: String,
    val date: String,
    val title: String,
    val excerpt: String,
    val vote_count: Int,
    val link: String,
    val categories: Array<Int>
)